<table>
  <tr>
    <th>Tipo</th>
    <th>Cantidad</th>
    <th>Producto</th>
  </tr>
  <a href="/kardex/crear">Insertar</a>
  <% registros.forEach(function (registro) { %>
  <tr>
    <% if(registro.tipo == 0 ) { %>
    <th>Ingreso</th>
    <% } else if(registro.tipo == 1 ) { %>
    <th>Salida</th>
    <% } %>
    <th><%= registro.cantidad %></th>
    <% if(registro.idProducto) { %>
    <th><%= registro.idProducto.nombreProducto %></th>
    <th><a href="/kardex/form/eliminar/<%= registro.idProducto.idProducto %>">Eliminar</a></th>
    <% } %>

  </tr>
  <% }) %>

</table>
